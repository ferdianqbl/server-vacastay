<div class="tab-pane fade show active" id="show-feature" role="tabpanel" aria-labelledby="home-tab">
  <!-- DataTales Example -->
  <div class="card shadow mb-4 mt-2">
    <div class="card-body">
      <div class="row">
        <div class="col-md-5">
          <form action="/admin/bank" method="POST" enctype="multipart/form-data">
            <div class="modal-body">
              <div class="form-group">
                <label for="featureName">Feature Name</label>
                <input type="text" class="form-control" id="featureName" placeholder="Enter Feature Name"
                  name="featureName" required />
              </div>
              <div class="form-group">
                <label for="qty">QTY</label>
                <input type="number" class="form-control" id="qty" placeholder="Enter Account Number" name="qty"
                  required />
              </div>
              <div class="form-group">
                <label for="image">Image</label>
                <input type="file" class="form-control" id="image" name="image" required />
              </div>
            </div>
            <div class="modal-footer justify-content-start">
              <button type="submit" class="btn btn-primary">Save</button>
            </div>
          </form>
        </div>

        <div class="col-md-7">
          <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
              <thead>
                <tr>
                  <th>No</th>
                  <th>Feature Name</th>
                  <th>QTY</th>
                  <th>Image URL</th>
                  <th>Action</th>
                </tr>
              </thead>
              <tbody>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>